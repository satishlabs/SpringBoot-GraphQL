type Query {
	firstQuery : String
	secondQuery: String
	fullName(firstName : String, lastNane : String) : String
	fullName1(sampleRequest : SampleRequest) : String
	student(id : Long) : StudentResponse
}

input SampleRequest{
	firstName : String!
	lastName : String
}

type StudentResponse{
	id : Long
	firstName : String
	lastName : String
	email : String
	street : String
	city : String
}